{
  "env": {
    "browser": true,
    "es2017": true //es8に沿って
  },
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "parserOptions": {
    // "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "globals": {
    "utils": "readonly",
    "jQuery": "readonly",
    "$": "readonly"
  },
  "rules": {
    // "semi": ["error", "never"],
    "no-undef": 0,
    "semi": 0
  }
}
